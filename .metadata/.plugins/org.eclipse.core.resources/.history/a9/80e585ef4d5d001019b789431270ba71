<!-- SQL definition -->
<mapper namespace="UserMapper">

<!-- 컬럼명이랑 필드명이 다르면 user안담긴다...AS를 이용해서 Alias를 사용 -->
	<select id="getUserList" resultMap="user">
		SELECT
		user_id AS userId,
		user_name AS userName,
		password,
		age,
		grade,
		reg_date AS regDate
		FROM users
	
	</select>
</mapper>
